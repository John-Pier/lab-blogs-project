{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "importHelpers": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "paths": {},
    "target": "es2017",
    "module": "esnext",
    "lib": ["es2021", "dom"]
  },
  "exclude": ["node_modules", "tmp"]
}
